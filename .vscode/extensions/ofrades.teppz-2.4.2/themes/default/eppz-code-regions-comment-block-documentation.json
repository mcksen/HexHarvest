{
	"name": "eppz! Code (comment.block.documentation)",
	"include": "eppz-code-markdown.json",
	"settings": [
		{
			"name": "Documentation (text)",
			"scope": [
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#7a7a78"
			}
		},
		{
			"name": "Documentation (punctuation, tags, attributes)",
			"scope": [
				"comment.block.documentation punctuation",
				"comment.block.documentation punctuation.definition.comment",
				"comment.block.documentation meta.tag punctuation",
				"comment.block.documentation meta.tag entity"
			],
			"settings": {
				"foreground": "#4f4f51"
			}
		},
		{
			"name": "Documentation (strings)",
			"scope": [
				"comment.block.documentation meta.tag string",
				"comment.block.documentation meta.tag string.quoted.single",
				"comment.block.documentation meta.tag string.quoted.double"
			],
			"settings": {
				"foreground": "#a39375"
			}
		}
	]
}